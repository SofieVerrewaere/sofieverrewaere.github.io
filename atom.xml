---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Tom Van de Wiele</title>
 <link href="http://ttvand.github.io/atom.xml" rel="self"/>
 <link href="http://ttvand.github.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://ttvand.github.io/</id>
 <author>
   <name>Tom Van de Wiele</name>
   <email>tvdwiele@gmail.com</email>
 </author>

 {% for post in site.posts %}
   <entry>
     <title>{{ post.title }}</title>
     <link href="http://thejqr.com{{ post.url }}"/>
     <updated>{{ post.date | date_to_xmlschema }}</updated>
     <id>http://thejqr.com{{ post.id }}</id>
     <content type="html">{{ post.content | xml_escape }}</content>
   </entry>
 {% endfor %}

</feed>
